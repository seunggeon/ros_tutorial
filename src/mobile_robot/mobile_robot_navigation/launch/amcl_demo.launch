<?xml version="1.0"?>
<launch>

        <!--Run the map server-->
        <arg name="map_file" default="$(find mobile_robot_gmapping)/map/my_map.yaml"/>
        <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />
        
        <!--Run AMCL-->
        <include file="$(find mobile_robot_navigation)/launch/amcl.launch" />
</launch>